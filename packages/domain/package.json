{
  "name": "@mojaloop/quoting-bc-domain",
  "version": "0.0.1",
  "description": "Quoting BC domain.",
  "license": "Apache-2.0",
  "homepage": "https://github.com/mojaloop/quoting-bc/tree/main/packages/domain",
  "bugs": {
    "url": "https://github.com/mojaloop/quoting-bc/issues#readme"
  },
  "contributors": [
    "Gon√ßalo Garcia <goncalogarcia99@gmail.com>",
    "Jason Bruwer <jason.bruwer@coil.com>",
    "Jose Antunes <jose.antunes@arg.software>",
    "Pedro Sousa Barreto <pedrob@crosslaketech.com>",
    "Rui Rocha <rui.rocha@arg.software>"
  ],
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean:dist": "rm -rf dist",
    "clean:node-modules": "rm -rf node_modules",
    "clean:dist-and-node-modules": "rm -rf dist node_modules",
    "clean": "npm run clean:dist",
    "audit:check": "npm npm audit",
    "dep:check": "npm exec ncu -- -e 2",
    "dep:update": "npm exec ncu -- -u",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test:unit": "jest --passWithNoTests --testMatch **/test/unit/**/*.test.ts ",
    "test:integration": "jest --passWithNoTests --testMatch **/test/integration/**/*.test.ts",
    "test:coverage": "jest --passWithNoTests --coverage --coverageThreshold='{}' --testMatch **/test/unit/**/*.test.ts",
    "test:coverage-check": "jest --coverage --testMatch **/test/unit/**/*.test.ts"
  },
  "dependencies": {
    "@mojaloop/logging-bc-public-types-lib": "^0.1.13",
    "@mojaloop/platform-shared-lib-messaging-types-lib": "^0.2.24",
    "@mojaloop/platform-shared-lib-public-messages-lib": "^0.1.24"
  }
}
